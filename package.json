{
  "name": "webknot-campus-reporting",
  "version": "1.0.0",
  "description": "Campus Event Reporting System - Admin Portal + Student App Backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
